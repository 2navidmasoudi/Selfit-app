{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "object-curly-newline": "off",
    "no-use-before-define": 0,
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/img-has-alt": [0],
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "linebreak-style":"off"
  },
  "globals": {
    "navigator": true,
    "fetch": true
  }
}
